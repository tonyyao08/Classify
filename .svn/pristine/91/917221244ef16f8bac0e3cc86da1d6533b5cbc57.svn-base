import time
from datetime import datetime

starttime = time.time()
interval = 5
while True:
    # Textual month, day and year
    now = datetime.now()
    t = time.time()
    d = now.strftime("%B_%d_%Y_%H:%M:%S")
    #current_time = time.strftime('%d_%m_%Y %H:%M:%S %Z', time.localtime(t))
    print(f"Current = {d}")
    #time.sleep(interval - ((time.time() - starttime) % interval))
    time.sleep(interval)
